{
    "manifest_version": 2,
  
    "name": "Password Generator",
    "description": "Quick launch a random and secure password for you to use anywhere",
    "version": "1.0.0",
    "icons": {
      "128": "noish.png",
      "64": "noish.png",
      "32": "noish.png" },
    "browser_action": {
      "default_icon": "noish.png",
      "default_popup": "generator.html"
    },
    "permissions": ["activeTab"]
  }